{
    "version": "0.1.0",
    "tasks": [
        {
            "taskName": "install",
            "showOutput": "silent",
            "command": "go",
            "args": [
                "install",
                "-v",
                "./..."
            ],
            "isBuildCommand": true,
            "echoCommand": false
        },
        {
            "taskName": "run",
            "showOutput": "always",
            "isShellCommand": true,
            "command": "${workspaceRootFolderName}"
        }
    ]
}